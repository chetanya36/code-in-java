public class Main
{
    public static boolean Gate(boolean input1,boolean input2,boolean input3,boolean input4)
    {
        if(input1 == true && input4 == true)
        {
            return true;
        }
        else if(input1 == true && (input2==true && input3==true))
        {
            return true;
        }
            return false;
    }
	public static void main(String[] args) {
		System.out.println(Gate(false,true,true,true));
	}
}
