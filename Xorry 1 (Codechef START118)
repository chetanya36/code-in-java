import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc =new Scanner(System.in);
		int n = sc.nextInt();
		for(int i=0;i<n;i++){
		    int x = sc.nextInt();
		    int temp =1,count =1;
		    int k = x;
		    while(x!=1){
		        temp*=2;
		        x = x>>1;
		    }
		    int k1 = k^temp;
		    int k2 = k&temp;
		    System.out.println(k1+" "+k2);
		}
	}
}
